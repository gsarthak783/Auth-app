{
  "name": "@gsarthak783/accesskit-auth",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript SDK for AccessKit Authentication System - Easy auth integration for any project",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "accesskit",
    "authentication",
    "auth",
    "sdk",
    "javascript",
    "typescript",
    "login",
    "signup",
    "user-management",
    "session",
    "jwt",
    "oauth",
    "security"
  ],
  "author": "AccessKit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gsarthak783/Auth-app",
    "directory": "auth-system/sdk"
  },
  "homepage": "https://access-kit-server.vercel.app",
  "bugs": {
    "url": "https://github.com/gsarthak783/Auth-app/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "jest": "^29.7.0",
    "rollup": "^4.6.1",
    "typescript": "^5.3.2",
    "rollup-plugin-dts": "^6.1.0"
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
} 